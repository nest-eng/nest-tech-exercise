{
  "name": "nest-portal-app",
  "version": "0.0.78",
  "private": true,
  "scripts": {
    "install-dev-certs": "mkcert -install && mkdir -p certs && mkcert -cert-file certs/ssl.pem -key-file certs/key.pem localhost",
    "dev": "npm run install-dev-certs && PORT=8080 node dev.server.js",
    "build": "next build && next export",
    "start": "next start -p ${PORT:=8080}",
    "test": "jest",
    "check-types": "tsc --noEmit",
    "format": "prettier --write .",
    "postinstall": "npx next telemetry disable"
  },
  "dependencies": {
    "@apollo/client": "^3.6.8",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "@mui/x-data-grid": "^5.5.1",
    "date-fns": "^2.28.0",
    "graphql": "^16.3.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.33.0",
    "recoil": "^0.7.3"
  },
  "devDependencies": {
    "@jest/types": "^28.1.1",
    "@svgr/webpack": "^6.2.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^28.1.2",
    "@types/lodash": "^4.14.178",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.5.1",
    "regenerator-runtime": "^0.13.9",
    "ts-jest": "^28.0.5",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix"
  }
}
