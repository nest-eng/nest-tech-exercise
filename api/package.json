{
  "name": "nest-tech-exercise-api",
  "version": "0.1.0",
  "description": "Nest Tech Exercise",
  "author": "Nest Health Tech Inc.",
  "private": true,
  "license": "UNLICENSED",
  "keywords": [
    "nestjs",
    "prisma",
    "prisma client",
    "typescript",
    "graphql"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write .",
    "install-dev-certs": "mkcert -install && mkdir -p certs && mkcert -cert-file certs/ssl.pem -key-file certs/key.pem localhost",
    "start": "nest start",
    "start:dev": "npm run install-dev-certs && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev --preview-feature && npm run prisma:generate",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature --force",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:db:push:dev": "npx prisma db push",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "prisma db seed",
    "postinstall": "npm run prisma:generate",
    "docker:build": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml build",
    "docker:dev": "npm run install-dev-certs && docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:db:push:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml exec nest-tech-exercise-api npm run prisma:db:push:dev",
    "docker:db:migrate:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml exec nest-tech-exercise-api npm run migrate:dev",
    "docker:db:migrate:dev:create": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml exec nest-tech-exercise-api npm run migrate:dev:create",
    "docker:db:seed": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml exec nest-tech-exercise-api npm run seed",
    "docker:db:reset": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml exec nest-tech-exercise-api npm run migrate:reset",
    "docker:prisma:studio": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml exec nest-tech-exercise-api npm run prisma:studio",
    "docker:prisma:generate": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml exec nest-tech-exercise-api npm run prisma:generate",
    "check-types": "tsc --noEmit",
    "generate-graphql-typings": "ts-node src/generate-graphql-typings.ts"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.2",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/graphql": "^10.0.9",
    "@nestjs/platform-express": "^8.4.4",
    "@ngneat/falso": "^6.0.2",
    "@prisma/client": "^3.12.0",
    "apollo-server-express": "^3.6.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^16.3.0",
    "graphql-scalars": "^1.17.0",
    "graphql-type-json": "^0.3.2",
    "lodash": "^4.17.21",
    "nestjs-prisma": "^0.16.0",
    "rxjs": "^7.5.4",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/testing": "^8.4.4",
    "@types/chance": "^1.1.3",
    "@types/jest": "^28.1.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "chance": "^1.1.8",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "jest": "^28.1.1",
    "jest-mock-extended": "^2.0.6",
    "prettier": "^2.6.2",
    "prisma": "^3.12.0",
    "prisma-nestjs-graphql": "^16.0.1",
    "supertest": "^6.2.2",
    "ts-jest": "^28.0.5",
    "ts-morph": "^15.1.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix"
  }
}
